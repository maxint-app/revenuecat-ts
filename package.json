{
  "name": "revenuecat-ts",
  "version": "2.0.0",
  "description": "RevenueCat API client for Typescript",
  "main": "dist/esm/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsc && npx tsc --module CommonJs --moduleResolution Node --outDir dist/cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxint-app/revenuecat-ts.git"
  },
  "keywords": [
    "revenuecat"
  ],
  "author": "Maxint Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxint-app/revenuecat-ts/issues"
  },
  "homepage": "https://github.com/maxint-app/revenuecat-ts#readme",
  "devDependencies": {
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5"
  }
}